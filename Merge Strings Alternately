1768.Merge Strings Alternately
class Solution {
public:
    string mergeAlternately(string word1, string word2) {
        string s="";
        int a=word1.length();
        int b=word2.length();
        int maxlen=max(a,b);
        for(int i=0;i<a||i<b;i++){
            if(i<a)
            s+=word1[i];
            if(i<b)
            s+=word2[i];
        }
        return s;
        
    }
};
